{
  "name": "nest-babel-starter",
  "version": "1.0.0",
  "description": "Nest Babel starter repository",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "start": "npx babel-node index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.1.0",
    "@nestjs/microservices": "^6.1.0",
    "@nestjs/platform-express": "^6.1.0",
    "@nestjs/websockets": "^6.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@nestjs/testing": "^6.0.0",
    "babel-loader": "^8.0.6",
    "jest": "^24.0.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^4.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "src",
    "testRegex": ".spec.js$",
    "coverageDirectory": "../coverage"
  }
}
